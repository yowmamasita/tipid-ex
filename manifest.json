{
    "content_scripts": [
        {
            "matches": [
                "http://*.tipidpc.com/*"
            ],
            "run_at": "document_idle",
            "js": [
                "js/lib/jquery-1.8.2.min.js",
                "js/content-script.js"
            ],
            "css": [
                "css/style.css"
            ]
        }
    ],
    "update_url": "http://myhost.com/api/extensions/updates.xml",
    "name": "Chrome Extension Template",
    "manifest_version": 2,
    "version": "0.1",
    "options_page": "options.html",
    "background": {
        "scripts": [
            "js/lib/jquery-1.8.2.min.js"
        ]
    },
    "permissions": [
    ],
    "description": "Chrome Extension Template for starting new extensions."
}
